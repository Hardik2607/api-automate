{
	"info": {
		"_postman_id": "094faa0e-7fab-4732-a60a-e28af7fad355",
		"name": "User",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "Fetch the list of Users",
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "{{api_url}}/user/get",
					"host": [
						"{{api_url}}"
					],
					"path": [
						"user",
						"get"
					]
				}
			},
			"response": []
		},
		{
			"name": "Create User",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"let random = +new Date();",
							"",
							"pm.globals.set(\"username\", `${random}-user`);",
							"pm.globals.set(\"email\", `${random}-user@test.com`);"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"url": {
					"raw": "{{api_url}}/user/create?username=hardik&email=abc@123.com&password=123456",
					"host": [
						"{{api_url}}"
					],
					"path": [
						"user",
						"create"
					],
					"query": [
						{
							"key": "username",
							"value": "hardik"
						},
						{
							"key": "email",
							"value": "abc@123.com"
						},
						{
							"key": "password",
							"value": "123456"
						}
					]
				}
			},
			"response": []
		}
	]
}